{{#each blogs as |blog| }}

<div class="row align-center mb-5">

    <a href="/editblog?id={{blog.id}}">
        <div class="col-md-6">
            <h2>{{blog.title}}</h2>

        </div>
        <div class="col-md-6">
            <h3>{{blog.created_date}}</h3>
        </div>
    </a>
</div>
{{/each}}